FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      SSS;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    building
    {
        type            zeroGradient;
    }
    ground
    {
        type            zeroGradient;
    }
    left
    {
        type            symmetry;
    }
    right
    {
        type            symmetry;
    }
	
	top
    {
        type            symmetry;
    }
}


